{
  "style": "klokantech-basic",
  "latitude": #(latitude),
  "longitude": #(longitude),
  "zoom": #(zoom),
  "width": 500,
  "height": 250,
  "scale": 1,
  "markers": [
    {
      "url": "#(img)",
      "height": 40,
      "width": 40,
      "x_offset": 0,
      "y_offset": 0,
      "latitude": #(latitude),
      "longitude": #(longitude)
    }
  ],
  "circles": [
    {
      "fill_color": "rgba(46,204,113,0.3)",
      "stroke_color": "rgba(46,204,113)",
      "stroke_width": 2,
      "radius": #(distance),
      "latitude": #(latitude),
      "longitude": #(longitude)
    }
  ]
}
